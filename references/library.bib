publisher = {American Chemical Society},
title = {{Computer Modeling Analysis for Enediyne Chromophore-Apoprotein Complex of Macromolecular Antitumor Antibiotic C-1027}},
url = {http://pubs.acs.org/doi/abs/10.1021/jm00041a004},
volume = {37},
year = {1994}
}
