publisher = {Elsevier},
title = {{Hydrophilic interaction chromatography (HILIC) in the analysis of antibiotics}},
url = {http://www.sciencedirect.com/science/article/pii/S0731708513001611?via{\%}3Dihub},
volume = {87},
year = {2014}
}
