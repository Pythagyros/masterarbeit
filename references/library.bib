title = {{Some Characteristics of DNA Strand Scission by Macromolecular Antitumor Antibiotic C-1027 Containing a Novel Enediyne Chromophore}},
url = {http://pubs.acs.org/doi/abs/10.1021/bi00072a008},
volume = {32},
year = {1993}
}
