issn = {0021-9193},
journal = {Journal of bacteriology},
month = {apr},
number = {7},
pages = {2006--16},
pmid = {8458843},
publisher = {American Society for Microbiology},
title = {{A gene cluster involved in aerial mycelium formation in Streptomyces griseus encodes proteins similar to the response regulators of two-component regulatory systems and membrane translocators.}},
