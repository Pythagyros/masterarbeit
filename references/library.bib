keywords = {Antibiotics,Antineoplastic,Antineoplastic: chemistry,Antineoplastic: pharmacology,Antineoplastic: therapeutic use,Chemical,DNA,Drug Design,Enediynes,Enediynes: chemistry,Enediynes: pharmacology,Enediynes: therapeutic use,Humans,Models,Molecular Structure,Neoplasm,Neoplasm: chemistry,Neoplasm: metabolism,Neoplasms,Neoplasms: drug therapy,Neoplasms: genetics,Neoplasms: pathology},
