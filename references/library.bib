journal = {The Journal of antibiotics},
number = {5},
pages = {791--802},
title = {{Conformation Studies on and Assessment by Spectral-Analysis of the Protein-Chromophore Interaction of the Macromolecular Antitumor Antibiotic C-1027}},
url = {https://www.jstage.jst.go.jp/article/antibiotics1968/46/5/46{\_}5{\_}791/{\_}pdf http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=8514634{\&}retmode=ref{\&}cmd=prlinks},
volume = {46},
