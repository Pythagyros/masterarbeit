@article{Mourey1984,
abstract = {The operation principles of an evaporative light-scattering detector for nonvolatile solutes are described in terms of nebulization and angular-light-scattering theories. Detector response increases sigmoldally with increasing sample concentration, which is predicted by concentration-dependent changes in aerosol particle size distributions. Optimum detector response can be obtained by changing the nebulized droplet size distribution via nebulizer-gas flow rate, sample size, or eluent adjustments. The aerosol light-scattering intensities can be corrected to give equivalent mass responses for several vinyl polymers of known densities and refractive indexes. Mass response is also obtained for polystyrene latices with diameters {\textless}0.3 $\mu$m. {\textcopyright} 1984 American Chemical Society.},
