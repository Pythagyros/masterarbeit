isbn = {1367-4803},
issn = {13674803},
journal = {Bioinformatics},
number = {15},
pages = {1972--1973},
pmid = {19505945},
title = {{trimAl: A tool for automated alignment trimming in large-scale phylogenetic analyses}},
url = {http://trimal.cgenomics.org/{\_}media/trimal.2009.pdf},
volume = {25},
year = {2009}
}
